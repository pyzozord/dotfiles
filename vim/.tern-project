{
  "ecmaVersion": 7,
  "libs": [
    "ecma5",
    "ecma6",
    "browser",
    "jquery",
    "browser",
    "underscore",
    "lodash",
    "react"
  ],
  "loadEagerly": [],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "requirejs": {
      "baseURL": "./",
      "paths": {}
    },
    "webpack": {
      "configPath": "./webpack.config.js"
    },
    "complete_strings": {
      "maxLength": 15
    }
  }
}
